{
    "default": {
        "identifier": "default",
        "description": "Capability for the bioparallel window",
        "local": true,
        "windows": ["bioparallel"],
        "permissions": [
            "core:path:default",
            "core:event:default",
            "core:window:default",
            "core:app:default",
            "core:image:default",
            "core:resources:default",
            "core:menu:default",
            "core:tray:default",
            {
                "identifier": "fs:allow-exists",
                "allow": ["**", "**/*", "/**/*"]
            },
            "fs:allow-read-file",
            {
                "identifier": "fs:allow-read-text-file",
                "allow": ["**", "**/*", "/**/*"]
            },
            "fs:allow-write-text-file",
            "store:allow-set",
            "store:allow-get",
            "store:allow-save",
            "store:allow-delete",
            "dialog:allow-open",
            "dialog:allow-save",
            "dialog:allow-message",
            "dialog:allow-ask",
            "dialog:allow-confirm",
            "store:default",
            "dialog:default",
            "fs:default"
        ]
    }
}
